    <validationRules>
        <fullName>Proper_Case</fullName>
        <active>true</active>
        <description>Airport name should be proper case</description>
        <errorConditionFormula>NOT( 
OR( 
BEGINS( Airport_Name__c ,&quot;A&quot;), 
BEGINS( Airport_Name__c ,&quot;B&quot;), 
BEGINS( Airport_Name__c ,&quot;C&quot;), 
BEGINS( Airport_Name__c ,&quot;D&quot;), 
BEGINS( Airport_Name__c ,&quot;E&quot;), 
BEGINS( Airport_Name__c ,&quot;F&quot;), 
BEGINS( Airport_Name__c ,&quot;G&quot;), 
BEGINS( Airport_Name__c ,&quot;H&quot;), 
BEGINS( Airport_Name__c ,&quot;I&quot;), 
BEGINS( Airport_Name__c ,&quot;J&quot;), 
BEGINS( Airport_Name__c ,&quot;K&quot;), 
BEGINS( Airport_Name__c ,&quot;L&quot;), 
BEGINS( Airport_Name__c ,&quot;M&quot;), 
BEGINS( Airport_Name__c ,&quot;N&quot;), 
BEGINS( Airport_Name__c ,&quot;O&quot;), 
BEGINS( Airport_Name__c ,&quot;P&quot;), 
BEGINS( Airport_Name__c ,&quot;Q&quot;), 
BEGINS( Airport_Name__c ,&quot;R&quot;), 
BEGINS( Airport_Name__c ,&quot;S&quot;), 
BEGINS( Airport_Name__c ,&quot;T&quot;), 
BEGINS( Airport_Name__c ,&quot;U&quot;), 
BEGINS( Airport_Name__c ,&quot;V&quot;), 
BEGINS( Airport_Name__c ,&quot;W&quot;), 
BEGINS( Airport_Name__c ,&quot;X&quot;), 
BEGINS( Airport_Name__c ,&quot;Y&quot;), 
BEGINS( Airport_Name__c ,&quot;Z&quot;) 
))</errorConditionFormula>
        <errorMessage>Please capitalize first words of the Airport Name</errorMessage>
    </validationRules>
