        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(FSTR__PCE_Step_Reassignment__r.FSTR__Interval_Unit__c, &quot;Days&quot;),(NOW() - FSTR__Assignment_Time__c) &gt; FSTR__PCE_Step_Reassignment__r.FSTR__Interval__c, IF(ISPICKVAL(FSTR__PCE_Step_Reassignment__r.FSTR__Interval_Unit__c, &quot;Minutes&quot;), ((NOW() - FSTR__Assignment_Time__c) * 1440)&gt; FSTR__PCE_Step_Reassignment__r.FSTR__Interval__c,
IF(ISPICKVAL(FSTR__PCE_Step_Reassignment__r.FSTR__Interval_Unit__c, &quot;Hours&quot;), ((NOW() - FSTR__Assignment_Time__c) * 24)&gt; FSTR__PCE_Step_Reassignment__r.FSTR__Interval__c,
False)
))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
