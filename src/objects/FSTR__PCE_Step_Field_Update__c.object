    <fields>
        <fullName>FSTR__IsChildStepDestination__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>/* Determines whether this is update has a child step destination. Handles values with __c or __r */

        IF(FSTR__Parent_Step_Name__c != null,
            OR(
                CONTAINS(LOWER(FSTR__Object_Name__c), LOWER(FSTR__Parent_Step_Name__c)),
                AND(
                    OR(
                        CONTAINS(LOWER(FSTR__Parent_Step_Name__c), &quot;__c&quot;),
                        CONTAINS(LOWER(FSTR__Parent_Step_Name__c), &quot;__r&quot;)
                    ),
                    CONTAINS(LOWER(FSTR__Object_Name__c), LOWER(LEFT(FSTR__Parent_Step_Name__c, LEN(FSTR__Parent_Step_Name__c) - 3)))
                )
            ), false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Child Step Destination</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>FSTR__IsChildStepSource__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>/* Determines whether this is update has a child step source. Handles values with __c or __r */

        IF(FSTR__Parent_Step_Name__c != null,
            OR(
                CONTAINS(LOWER(FSTR__Source_Object__c), LOWER(FSTR__Parent_Step_Name__c)),
                AND(
                    OR(
                        CONTAINS(LOWER(FSTR__Parent_Step_Name__c), &quot;__c&quot;),
                        CONTAINS(LOWER(FSTR__Parent_Step_Name__c), &quot;__r&quot;)
                    ),
                    CONTAINS(LOWER(FSTR__Source_Object__c), LOWER(LEFT(FSTR__Parent_Step_Name__c, LEN(FSTR__Parent_Step_Name__c) - 3)))
                )
            ), false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Child Step Source</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
