    <validationRules>
        <fullName>Cannot_change_record_type</fullName>
        <active>false</active>
        <description>You cannot change the record type for this Solution Status.</description>
        <errorConditionFormula>AND( RecordType.DeveloperName = &quot;Visible&quot;, 
     OR(ISPICKVAL( Solution_Status__c , &quot;Available&quot;), 
        ISPICKVAL( Solution_Status__c, &quot;Unavailable&quot;),
        ISPICKVAL(Solution_Status__c, &quot;Requested&quot;),
        ISPICKVAL(Solution_Status__c, &quot;On Hold for Non-AvP Client&quot;))
)</errorConditionFormula>
        <errorDisplayField>Solution_Status__c</errorDisplayField>
        <errorMessage>You cannot change the record type for this Solution Status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Aircraft_Confirmed_Wet_Runway</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(  Solution_Status__c  , &quot;Available&quot;), ISPICKVAL ( Solution_Category__c , &quot;Non-Guaranteed/Charter&quot;),
ISBLANK( TEXT( Operator_Confirmed_Aircraft__c  )))</errorConditionFormula>
        <errorDisplayField>Operator_Confirmed_Aircraft__c</errorDisplayField>
        <errorMessage>Please enter Operator Confirmed Aircraft Wet Runway</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Broker_Fee</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(  Solution_Status__c  , &quot;Available&quot;), ISPICKVAL ( Solution_Category__c , &quot;Non-Guaranteed/Charter&quot;), NOT(ISBLANK( Broker__c )),
ISBLANK(  Broker_Fee__c  ))</errorConditionFormula>
        <errorDisplayField>Broker_Fee__c</errorDisplayField>
        <errorMessage>Please enter Broker Fee</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Crew_Pass</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(  Solution_Status__c  , &quot;Available&quot;), ISPICKVAL ( Solution_Category__c , &quot;Non-Guaranteed/Charter&quot;),
ISBLANK(TEXT(  Will_Crew_Pass_ARGUS_or_Wyvern__c  )))</errorConditionFormula>
        <errorDisplayField>Will_Crew_Pass_ARGUS_or_Wyvern__c</errorDisplayField>
        <errorMessage>Please enter Will crew pass Argus or Wyvern</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Estimated_Billable_Time</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(  Solution_Status__c  , &quot;Available&quot;), ISPICKVAL ( Solution_Category__c , &quot;Non-Guaranteed/Charter&quot;),
ISBLANK(  Estimated_Billable_Time_hours__c  ))</errorConditionFormula>
        <errorDisplayField>Estimated_Billable_Time_hours__c</errorDisplayField>
        <errorMessage>Please enter estimated billable time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Estimated_Cost</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(  Solution_Status__c  , &quot;Available&quot;), ISPICKVAL ( Solution_Category__c , &quot;Non-Guaranteed/Charter&quot;),
ISBLANK( Estimated_Cost__c ))</errorConditionFormula>
        <errorDisplayField>Estimated_Cost__c</errorDisplayField>
        <errorMessage>Please enter estimated operator cost</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Operator_Confirmed_Nonstop</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL(  Solution_Status__c  , &quot;Available&quot;), ISPICKVAL ( Solution_Category__c , &quot;Non-Guaranteed/Charter&quot;),
ISBLANK( TEXT( Operator_Confirmed_Nonstop__c  )))</errorConditionFormula>
        <errorDisplayField>Operator_Confirmed_Nonstop__c</errorDisplayField>
        <errorMessage>Please enter Operator Confirmed Nonstop</errorMessage>
    </validationRules>
    <validationRules>
